                          <GradientStop Color="#FFB9F0FF" />
                          <GradientStop Color="#FF779CB1" Offset="0.478" />
                          <GradientStop Color="#FBA6B6B9" Offset="0.418" />
                        </LinearGradientBrush>
                      </Grid.Background>
                      <!--ContentPresenter-->
                      <ContentPresenter Grid.Column="1" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Content="{TemplateBinding Content}" Margin="0,4" />
                    </Grid>
