                        <GradientStop Color="WhiteSmoke" />
                        <GradientStop Color="#7F0081B8" Offset="1" />
                      </LinearGradientBrush>
                    </Border.BorderBrush>
                    <!--LawBotGlossyButtonContentGrid-->
                    <Grid x:Name="LawBotGlossyButtonContentGrid" Margin="2">
                      <Grid.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
