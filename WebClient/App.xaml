                          <EasingPointKeyFrame KeyTime="00:00:00" Value="0.499,0.001" />
                        </PointAnimationUsingKeyFrames>
                        <PointAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0010000" Storyboard.TargetName="LawBotGlossyButtonRootBorder" Storyboard.TargetProperty="(Border.BorderBrush).(LinearGradientBrush.EndPoint)">
                          <EasingPointKeyFrame KeyTime="00:00:00" Value="0.501,0.999" />
                        </PointAnimationUsingKeyFrames>
                        <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0010000" Storyboard.TargetName="LawBotGlossyButtonRootBorder" Storyboard.TargetProperty="(Border.BorderBrush).(GradientBrush.GradientStops)[0].(GradientStop.Color)">
                          <EasingColorKeyFrame KeyTime="00:00:00" Value="#00000724" />
                        </ColorAnimationUsingKeyFrames>
                        <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0010000" Storyboard.TargetName="LawBotGlossyButtonBorder" Storyboard.TargetProperty="(Border.BorderBrush).(GradientBrush.GradientStops)[3].(GradientStop.Color)">
                          <EasingColorKeyFrame KeyTime="00:00:00" Value="#FF000724" />
                        </ColorAnimationUsingKeyFrames>
                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0010000" Storyboard.TargetName="LawBotGlossyButtonContentGrid" Storyboard.TargetProperty="(Panel.Background).(GradientBrush.GradientStops)[3].(GradientStop.Offset)">
                          <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.357" />
                        </DoubleAnimationUsingKeyFrames>
                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0010000" Storyboard.TargetName="LawBotGlossyButtonContentGrid" Storyboard.TargetProperty="(Panel.Background).(GradientBrush.GradientStops)[2].(GradientStop.Offset)">
                          <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.596" />
                        </DoubleAnimationUsingKeyFrames>
                      </Storyboard>
                    </VisualState>
                    <!--VisualState Pressed-->
                    <VisualState x:Name="Pressed">
                      <Storyboard>
                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0010000" Storyboard.TargetName="LawBotGlossyButtonContentGrid" Storyboard.TargetProperty="(Panel.Background).(GradientBrush.GradientStops)[3].(GradientStop.Offset)">
                          <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.438" />
                        </DoubleAnimationUsingKeyFrames>
                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0010000" Storyboard.TargetName="LawBotGlossyButtonContentGrid" Storyboard.TargetProperty="(Panel.Background).(GradientBrush.GradientStops)[2].(GradientStop.Offset)">
                          <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.751" />
                        </DoubleAnimationUsingKeyFrames>
                      </Storyboard>
                    </VisualState>
                    <!--VisualState Disabled-->
                    <VisualState x:Name="Disabled">
                      <Storyboard>
                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0010000" Storyboard.TargetName="LawBotGlossyButtonTemplateMainGrid" Storyboard.TargetProperty="(UIElement.Opacity)">
                          <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.6" />
                        </DoubleAnimationUsingKeyFrames>
                        <ColorAnimation Duration="0" To="#FFA5EBFF" Storyboard.TargetProperty="(Panel.Background).(GradientBrush.GradientStops)[1].(GradientStop.Color)" Storyboard.TargetName="LawBotGlossyButtonContentGrid" d:IsOptimized="True" />
                        <ColorAnimation Duration="0" To="#FB9BB2B8" Storyboard.TargetProperty="(Panel.Background).(GradientBrush.GradientStops)[3].(GradientStop.Color)" Storyboard.TargetName="LawBotGlossyButtonContentGrid" d:IsOptimized="True" />
                        <ColorAnimation Duration="0" To="#FF53AFCE" Storyboard.TargetProperty="(Panel.Background).(GradientBrush.GradientStops)[2].(GradientStop.Color)" Storyboard.TargetName="LawBotGlossyButtonContentGrid" d:IsOptimized="True" />
                        <ColorAnimation Duration="0" To="#FF9ED1F3" Storyboard.TargetProperty="(Panel.Background).(GradientBrush.GradientStops)[0].(GradientStop.Color)" Storyboard.TargetName="LawBotGlossyButtonContentGrid" d:IsOptimized="True" />
                      </Storyboard>
                    </VisualState>
