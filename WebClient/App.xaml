                        <GradientStop Color="#FF679297" Offset="0.374" />
                        <GradientStop Color="#FE4B8A9F" Offset="0.678" />
                        <GradientStop Color="#FF006EB2" />
                      </LinearGradientBrush>
                    </Border.Background>
                    <Border.BorderBrush>
                      <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
