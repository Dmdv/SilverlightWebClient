          </Setter.Value>
        </Setter>
        <Setter Property="FontWeight" Value="Normal" />
        <Setter Property="Foreground" Value="#FF28334E" />
        <Setter Property="FontStyle" Value="Normal" />
        <Setter Property="FontSize" Value="13.333" />
