                      <GradientStop Color="#33FFFFFF" Offset="1" />
                    </LinearGradientBrush>
                  </Border.BorderBrush>
                  <!--LawBotGlossyButtonBorder-->
                  <Border x:Name="LawBotGlossyButtonBorder" BorderThickness="1" CornerRadius="5" Effect="{TemplateBinding Effect}">
