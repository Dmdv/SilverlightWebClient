<UserControl x:Class="WebClient.ICS.Client.Editors.BooleanEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ComboBox
        x:Name="BooleanEditorControl"
        SelectedItem="{Binding Data, Converter={StaticResource EditDataConverter}, ConverterParameter=BooleanEditorControl, Mode=TwoWay}" 
        VerticalAlignment="Bottom" Width="400" HorizontalAlignment="Left"/>

</UserControl>
